@model Heroes.Models.VillainsModel

<h1>@Model.villainName Update</h1>

@using (@Html.BeginForm("UpdateVillain", "Villains"))
{
    <partial name = "_villainsform" model = "@Model" />
    <div>
        @Html.HiddenFor(v => v.id)
    </div>

    
    <button type = "submit" class ="btn btn-outline-success" id="newBtn">Submit</button>
    
}